    <content><![CDATA[
vector<int> factors;

void getfactors(int n){
    /* TC : O(sqrt(n)) , factors not in a sorted order*/
    factors.clear();
    for(int i = 1; i * i <= n; i++){
        if(n % i == 0){
            factors.emplace_back(i);
            if(n/i == i) continue;
            factors.emplace_back(n/i);
        }
    }
}]]></content>
    <!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
    <tabTrigger>FACTORIZE</tabTrigger>
